hors.js","original":{"line":79,"column":31}},{"name":"colors","generated":{"line":100,"column":22},"source":"Components/Authors.js","original":{"line":90,"column":22}},{"name":"green","generated":{"line":100,"column":36},"source":"Components/Authors.js","original":{"line":90,"column":29}},{"generated":{"line":100,"column":41},"source":"Components/Authors.js","original":{"line":90,"column":35}},{"name":"shadows","generated":{"line":101,"column":22},"source":"Components/Authors.js","original":{"line":91,"column":22}},{"name":"card","generated":{"line":101,"column":37},"source":"Components/Authors.js","original":{"line":91,"column":30}},{"generated":{"line":101,"column":41},"source":"Components/Authors.js","original":{"line":91,"column":35}},{"name":"colors","generated":{"line":104,"column":21},"source":"Components/Authors.js","original":{"line":94,"column":21}},{"name":"white","generated":{"line":104,"column":35},"source":"Components/Authors.js","original":{"line":94,"column":28}},{"generated":{"line":104,"column":40},"source":"Components/Authors.js","original":{"line":94,"column":34}},{"generated":{"line":107,"column":1},"source":"Components/Authors.js","original":{"line":76,"column":0}}],"sources":{"Components/Authors.js":"import React from 'react'\nimport styled, { css } from 'styled-components'\n\n// Components\nimport { colors, shadows } from './theme'\nimport { Box3 } from './Boxes'\nimport { Button4 } from './Buttons'\nimport { Flex } from './Containers'\nimport { H3, P1 } from './Typography'\n\nconst Authors = ({\n    filter,\n    authors,\n}) => {\n    const [selectedUsername, setSelectedUsername] = React.useState(null)\n    const [selectedSender, setSelectedSender] = React.useState(null)\n\n    const onSelectedAuthor = (id) => {\n        let authorObject = authors.filter(function(author) {\n            return author.sender === id;\n        })\n\n        setSelectedUsername(authorObject[0].username)\n        setSelectedSender(authorObject[0].sender)\n    }\n\n    return (\n        <>\n            {filter === 'authors' && selectedUsername === null &&\n                (<>{\n                    authors === null\n                    ? <P1 center color={colors.white}>There are no authors.</P1>\n                    : (\n                        <Box3 marginTop={50}>\n                            <Flex wrap={'true'}>\n                                {authors.map(author => (\n                                    <AuthorCard\n                                        key={author.sender}\n                                        onSelectedAuthor={onSelectedAuthor}\n                                        sender={author.sender}\n                                        username={author.username}\n                                    />\n                                ))}\n                            </Flex>\n                        </Box3>\n                    )\n                }</>)\n            }\n            {filter === 'authors' && selectedUsername !== null && (\n                <Box3 marginTop={50}>\n                    <Button4 color={colors.yellow} onClick={() => setSelectedUsername(null)}>View all authors</Button4>\n                    <Box3 marginTop={50}>\n                        <H3 color={colors.white}>{selectedUsername}</H3>\n                        <P1 color={colors.white}>Account ID: {selectedSender}</P1>\n                    </Box3>\n                </Box3>\n            )}\n        </>\n    )\n}\n\nexport default Authors\n\nconst AuthorCard = ({\n    username,\n    sender,\n    onSelectedAuthor,\n}) => {\n    return (\n        <AuthorCardBackground onClick={onSelectedAuthor.bind(this, sender)}>\n            <P1 color={colors.black}>{username}</P1>\n        </AuthorCardBackground>\n    )\n}\n\nconst AuthorCardBackground = styled.div`\n    width: 20rem;\n    height: 8rem;\n    background: ${colors.white};\n    display: flex;\n    justify-content: center;\n    align-content: center;\n    margin-top: 2rem;\n    margin-right: 2rem;\n    border-radius: 5px;\n    transition: all .3s ease;\n\n    &:hover {\n        cursor: pointer;\n        background: ${colors.green};\n        box-shadow: ${shadows.card};\n\n        p {\n            color: ${colors.white};\n        }\n    }\n`\n"},"lineCount":null}},"error":null,"hash":"3af9ed6db0ee9db495339a770eca3d97","cacheData":{"env":{}}}